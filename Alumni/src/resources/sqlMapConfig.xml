<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<!-- Regsiter Model  -->
		<typeAlias alias="AlumniRegister" type="com.alumni.Model.AlumniRegisterModel" />
		<!-- Login Model -->
		<typeAlias alias="Login" type="com.alumni.Model.Login" />
		<typeAlias alias="AdminApprovalModel" type="com.alumni.Model.AdminApprovalModel" />
		<typeAlias alias="AdminEmployeeApprovalModel" type="com.alumni.Model.AdminEmployeeApprovalModel" />
		<typeAlias alias="PostjobModel" type="com.alumni.Model.PostjobModel" />
		<typeAlias alias="ViewJobModel" type="com.alumni.Model.ViewJobModel" />
		<typeAlias alias="EventsModel" type="com.alumni.Model.EventsModel" />
	</typeAliases>

	<mappers>
		<!-- Regsiter MyBatis -->
		<mapper resource="com/alumni/Alumni-Register-MyBatis.xml" />
		<!-- Login MyBatis -->
		<mapper resource="com/alumni/Alumni-Login-MyBatis.xml" />
		
		<mapper resource="com/alumni/Alumni-AdminApproval-MyBatis.xml" />
		<mapper resource="com/alumni/Alumni-AdminEmployeeApproval-MyBatis.xml" />
		<mapper resource="com/alumni/Alumni-Postjob-MyBatis.xml" />
		<mapper resource="com/alumni/Alumni-ViewJob-MyBatis.xml" />
		<mapper resource="com/alumni/Alumni-Events-MyBatis.xml" />
	</mappers>

</configuration>                                                       