<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AlumniDiscussion">

	<insert id="postDiscussion" parameterType="map">
		INSERT INTO
		alumni.discussion_title(title, std_id,status)
		VALUES
		(#{p.discussionTilte},
		#{p.std_id},TRUE);
	</insert>

</mapper>